{
  "name": "gemini-node-api",
  "version": "0.3.5",
  "description": "Gemini Node.js API",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "types": "index.d.ts",
  "engines": {
    "node": ">=10.16.3"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.28",
    "@types/ws": "^6.0.4",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "husky": "^3.1.0",
    "jsdoc": "^3.6.3",
    "lint-staged": "^9.5.0",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "lint": "eslint --ext .js ./",
    "prettier": "prettier -l *.{js,md,ts,json} **/*.{js,md,ts,json}",
    "prettier-write": "npm run prettier -- --write",
    "generate-docs": "jsdoc ./ -c jsdoc.json",
    "test": "mocha --full-trace --ui tdd --bail --reporter spec tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vansergen/gemini-node-api.git"
  },
  "keywords": [
    "Gemini",
    "crypto",
    "exchange",
    "API"
  ],
  "author": "Sergey Bakulin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vansergen/gemini-node-api/issues"
  },
  "homepage": "https://github.com/vansergen/gemini-node-api#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  }
}
